{% extends 'base.html.twig' %}

{% block body %}

    <div class="row">
        {% for image in images %}
            <div class="col-sm-6 col-md-4 ">
                <a href="{{ path('view') }}">
                    <img src="{{ asset('images/'~image) }}" class="img-thumbnail" alt="...">
                </a>
            </div>
        {% endfor %}
    </div>

    <div class="navigation">
        {{ knp_pagination_render(images) }}
    </div>


{% endblock %}
